# Public port exposed by docker-compose (host side)
STARTPAGE_PUBLIC_PORT=8080

# Session lifetime in seconds (default 12h)
DASH_SESSION_TTL=43200

# Icon index cache duration (seconds)
DASH_ICON_INDEX_TTL=21600

# Max icon search results returned by API
DASH_ICON_SEARCH_MAX_LIMIT=30

# Optional timezone (container)
TZ=UTC
